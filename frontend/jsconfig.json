{
    "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"]
      },
      "jsx": "react",
      "target": "es6",
      "module": "commonjs",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "moduleResolution": "node"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "build"]
  }